{% extends 'base.html.twig' %}

{% block title %}Session index{% endblock %}

{% block body %}
    <h1>Session index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>Libelles</th>
                <th>Despriptions</th>
                <th>Datedebut</th>
                <th>Datefin</th>
                <th>Createsds</th>
                <th>Updateds</th>
                <th>Enableds</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for session in sessions %}
            <tr>
                <td>{{ session.id }}</td>
                <td>{{ session.libelle }}</td>
                <td>{{ session.libelles }}</td>
                <td>{{ session.despriptions }}</td>
                <td>{{ session.datedebut ? session.datedebut|date('Y-m-d') : '' }}</td>
                <td>{{ session.datefin ? session.datefin|date('Y-m-d') : '' }}</td>
                <td>{{ session.createsds ? session.createsds|date('Y-m-d') : '' }}</td>
                <td>{{ session.updateds ? session.updateds|date('Y-m-d') : '' }}</td>
                <td>{{ session.enableds ? session.enableds|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_session_show', {'id': session.id}) }}">show</a>
                    <a href="{{ path('app_session_edit', {'id': session.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_session_new') }}">Create new</a>
{% endblock %}
